      "type": "datetime"
    },
    "tokenExpiresAt": {
      "type": "datetime"
    },
    "subscribedAt": {
      "type": "datetime"
    },
    "unsubscribedAt": {
      "type": "datetime"
    },
    "subscriptionState": {
      "type": "enumeration",
